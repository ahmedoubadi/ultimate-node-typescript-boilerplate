{
  "name": "ultimate-node-typescript-boilerplate",
  "version": "0.1.0",
  "description": "advanced project template to jump start a Node.js back-end application in TypeScript. ESLint, Jest and type definitions included.",
  "main": "src/index.ts",
  "scripts": {
    "dev": "nodemon",
    "start": "ts-node -r tsconfig-paths/register src/index.ts",
    "start:prod": "node dist/main.js",
    "build": "webpack --config webpack.config.js",
    "build:dev": "tsc -w",
    "test": "mocha -w -r ts-node/register src/**/*.test.ts",
    "format": "prettier --write \"**/*.ts\"",
    "lint": "eslint './src/'"
  },
  "author": "ahmed oubadi",
  "license": "ISC",
  "devDependencies": {
    "@types/express": "^4.17.20",
    "@types/node": "^20.8.7",
    "@typescript-eslint/eslint-plugin": "^6.9.0",
    "@typescript-eslint/parser": "^6.9.0",
    "eslint": "^8.52.0",
    "eslint-config-google": "^0.14.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-prettier": "^5.0.1",
    "prettier": "^3.0.3",
    "pretty-quick": "^3.1.3",
    "ts-loader": "^9.5.0",
    "ts-node": "^10.9.1",
    "tsconfig-paths": "^4.2.0",
    "typescript": "^5.2.2",
    "webpack": "^5.89.0",
    "webpack-cli": "^5.1.4",
    "webpack-node-externals": "^3.0.0"
  },
  "dependencies": {
    "body-parser": "^1.20.2",
    "compression": "^1.7.4",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "express-rate-limit": "^7.1.2",
    "zerg": "^2.1.2"
  }
}
